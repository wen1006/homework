#include <stdio.h>
#include<stdlib.h>
int main (){
  printf("請輸入一個數：");
  int n ,a=1;
  scanf("%d",&n);
  for (int i=2;i<=n;i++)
  a*=i;
  printf("該數的N!為：%d\n",a);
  system("pause");
  return 0;
}
